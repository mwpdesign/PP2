# API Configuration
VITE_API_URL=http://localhost:8000
VITE_WS_URL=ws://localhost:8000
VITE_ENV=development

# Authentication
VITE_JWT_SECRET=healthcare-ivr-development-secret-key-2025

# API Settings
VITE_API_TIMEOUT=30000

# WebSocket Settings
VITE_WS_RECONNECT_INTERVAL=5000
VITE_WS_MAX_RECONNECT_ATTEMPTS=5
VITE_WS_PING_INTERVAL=30000

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true

# Security Settings
VITE_ENFORCE_2FA=false
VITE_SESSION_TIMEOUT=3600000 # 1 hour in milliseconds

# Logging
VITE_LOG_LEVEL=debug # debug, info, warn, error

# =============================================================================
# DEVELOPMENT TEST CREDENTIALS (SECURE THESE!)
# =============================================================================
